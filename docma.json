{
  "src": [
    "./cornerstone-tools-src/**/*.js"
  ],
  "dest": "./cornerstone-doc/.vuepress/public/cornerstone-tools/latest/api",
  "app": {
    "title": "Cornerstone Tools: API Docs",
    "routing": "query",
    "base": "/cornerstone-book/cornerstone-tools/latest/api",
    "server": "github"
  },
  "jsdoc": {
    "hierarchy": true,
    "package": true,
    "private": false,
    "undescribed": false
  },
  "template": {
    "path": "zebra",
    "options": {
      "navbar": true,
      "sidebar": {
        "enabled": true,
        "outline": "tree"
      }
    }
  }
}